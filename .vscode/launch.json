{
	// IntelliSense を使用して利用可能な属性を学べます。
	// 既存の属性の説明をホバーして表示します。
	// 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
		},
		{
			"name": "Python: App File from config",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/app.py",
			"console": "integratedTerminal",
			"args": [
				"--target", "tests"
			]
		},
		{
			"name": "Python: App File auto discovery",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/app.py",
			"console": "integratedTerminal",
			"args": [
				"--target", "dialog",
				"--target", "quests",
				"--target", "items",
				"--discover"
			]
		},
		{
			"name": "Python: Test App File",
			"type": "python",
			"request": "launch",
			"module": "unittest",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": [
				"tests/test_app.py"
			]
		},
		{
			"name": "Python: Test TransWorker File",
			"type": "python",
			"request": "launch",
			"module": "unittest",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": [
				"tests/rogw/test_transworker.py"
			]
		},
		{
			"name": "Python: Test JsonQuery File",
			"type": "python",
			"request": "launch",
			"module": "unittest",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": [
				"tests/rogw/test_jsonquery.py"
			]
		},
		{
			"name": "Python: Test ModJson File",
			"type": "python",
			"request": "launch",
			"module": "unittest",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": [
				"tests/rogw/test_modjson.py"
			]
		}
	]
}