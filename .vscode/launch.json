{
	// IntelliSense を使用して利用可能な属性を学べます。
	// 既存の属性の説明をホバーして表示します。
	// 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"args": [
				"--dest", "dest/",
				"--keys", "description",
				"--files", "fu_assets/items/active/fishingrod/profishingrod.activeitem"
			],
			"envFile": "${workspaceFolder}/.env"
		},
		{
			"name": "Python: Test File",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tests/test_app.py",
			"console": "integratedTerminal",
			"args": [
				"-m", "unittest", "${workspaceFolder}/tests/test_app.py",
			],
			"envFile": "${workspaceFolder}/.env"
		}
	]
}